{
	"name": "@svelte.codes/modal-manager",
	"private": true,
	"version": "0.0.0",
	"svelte": "./dist/index.js",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "svelte-kit sync && svelte-package",
		"build:watch": "nodemon --watch src/**/* --exec \"npm run build\"",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/package": "^2.2.4",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@tsconfig/svelte": "^5.0.2",
		"@types/node": "^20.10.5",
		"nodemon": "^3.0.2",
		"rollup": "^4.9.1",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-svelte": "^7.1.6",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^5.0.8",
		"vite-plugin-dts": "^3.6.4"
	},
	"main": "./dist/modal-manager.umd.cjs",
	"module": "./dist/modal-manager.js",
	"types": "./dist/modal-manager.d.ts",
	"exports": {
		".": {
			"import": "./dist/modal-manager.js",
			"require": "./dist/modal-manager.umd.cjs"
		},
		"./DefaultOverlay.svelte": {
			"types": "./dist/DefaultOverlay.svelte.d.ts",
			"svelte": "./dist/DefaultOverlay.svelte"
		}
	}
}
